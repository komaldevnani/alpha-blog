<% flash.each do |name,msg| %>
    <div class='alert alert-<%="#{name}"%> alert-dismissible' role='alert'>
        <ul>
            <li><%= msg %></li>
        </ul>
        <button type="button" class="close" data-dismiss="alert" aria-label="close">
        <span aria-hidden="true">&times;</span>
        </button>
    </div>
<%end%>